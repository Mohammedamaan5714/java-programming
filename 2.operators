//assignment 
//comparison
//logical operator
public class A1_java {
    public static void main(String[] args) {
        int num1, num2, num3, num4, num5;

        // Arithmetic Operators
        num1 = 10 + 5;      // Addition
        System.out.println("Addition: " + num1);       // Output: Addition: 15

        num2 = 10 - 5;      // Subtraction
        System.out.println("Subtraction: " + num2);    // Output: Subtraction: 5

        num3 = 10 * 5;      // Multiplication
        System.out.println("Multiplication: " + num3); // Output: Multiplication: 50

        num4 = 10 / 5;      // Division
        System.out.println("Division: " + num4);       // Output: Division: 2

        num5 = 10 % 5;      // Modulus
        System.out.println("Modulus: " + num5);        // Output: Modulus: 0

        // Assignment Operators
        int x = 10;         // Assignment
        x += 5;             // Compound Assignment
        System.out.println("x: " + x);                 // Output: x: 15

        // Comparison Operators
        if (num1 == num2) {
            System.out.println("Equal");               // Output: (No Output - Condition not met)
        }
        if (num1 != num2) {
            System.out.println("Not equal");           // Output: Not equal
        }
        if (num1 > num2) {
            System.out.println("Greater than");        // Output: Greater than
        }
        if (num1 < num2) {
            System.out.println("Less than");           // Output: (No Output - Condition not met)
        }
        if (num1 >= num2) {
            System.out.println("Greater than or equal to");  // Output: Greater than or equal to
        }
        if (num1 <= num2) {
            System.out.println("Less than or equal to");     // Output: (No Output - Condition not met)
        }

        // Logical Operators
        boolean condition1 = true;
        boolean condition2 = false;

        if (condition1 && condition2) {
            System.out.println("Logical AND");          // Output: (No Output - Condition not met)
        }
        if (condition1 || condition2) {
            System.out.println("Logical OR");           // Output: Logical OR
        }
        if (!condition1) {
            System.out.println("Logical NOT");          // Output: (No Output - Condition not met)
        }

        // Increment and Decrement Operators
        int y = 5;
        y++;                // Increment
        System.out.println("Increment: " + y);          // Output: Increment: 6

        y--;                // Decrement
        System.out.println("Decrement: " + y);          // Output: Decrement: 5
    }
}

